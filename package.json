{
  "name": "my-education",
  "version": "1.0",
  "description": "Best project",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "scripts": {
    "start": "better-npm-run start",
    "start:test": "better-npm-run start:test",
    "start:prod": "better-npm-run start:prod",
    "build": "yarn clean && better-npm-run build",
    "clean": "better-npm-run clean",
    "analyze": "yarn clean && better-npm-run analyze"
  },
  "betterScripts": {
    "start": {
      "command": "nodemon .",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start:test": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "testing",
        "PORT": 3000
      }
    },
    "start:prod": {
      "command": "node .",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "clean": {
      "command": "rimraf ./public/*"
    },
    "analyze": {
      "command": "webpack -p --progress --hide-modules --config ./tools/webpack/config.babel.js",
      "env": {
        "NODE_ENV": "analyze"
      }
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3"
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "@loadable/babel-plugin",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-remove-console",
          "transform-react-remove-prop-types"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "plugins": [
      "react"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack/config.babel.js"
        }
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "linebreak-style": "off",
      "comma-dangle": "off",
      "no-trailing-spaces": "off",
      "padded-blocks": "off",
      "object-curly-newline": "off",
      "no-prototype-builtins": "off",
      "global-require": "off",
      "no-console": "off",
      "no-underscore-dangle": "off",
      "function-paren-newline": "off",
      "max-len": [
        "error",
        {
          "code": 500
        }
      ],
      "react/forbid-prop-types": "off",
      "react/require-default-props": "off",
      "react/destructuring-assignment": "off",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/no-cycle": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "specialLink": [
            "to"
          ]
        }
      ]
    },
    "globals": {
      "__CLIENT__": true,
      "__SERVER__": true,
      "__DEV__": true,
      "__TEST__": false
    }
  },
  "nodemonConfig": {
    "watch": [
      "src/server.js",
      "src/core/utils/renderHtml.js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@types/react": "^16.9.23",
    "accept-language": "^3.0.18",
    "arui-feather": "^18.2.3",
    "asset-require-hook": "^1.2.0",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.1",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "better-npm-run": "^0.1.1",
    "chalk": "^3.0.0",
    "classnames": "^2.2.6",
    "cn-decorator": "^2.1.0",
    "compression": "^1.7.4",
    "connected-react-router": "^6.7.0",
    "cookie-parser": "^1.4.4",
    "core-js": "3.6.4",
    "css-modules-require-hook": "^4.2.3",
    "date-fns": "^2.11.0",
    "dayjs": "^1.8.28",
    "express": "^4.17.1",
    "form-data": "^3.0.0",
    "formik": "^2.1.4",
    "helmet": "^3.21.3",
    "history": "^4.10.1",
    "hpp": "^0.2.2",
    "html-minifier": "^4.0.0",
    "immutable": "^4.0.0-rc.12",
    "js-cookie": "^2.2.0",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.14.1",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-date-range": "^1.0.1",
    "react-datepicker": "^4.2.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-intl": "^2.9.0",
    "react-markdown": "^5.0.3",
    "react-redux": "^7.2.1",
    "react-responsive-modal": "^4.0.1",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.27.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4",
    "serialize-javascript": "^3.0.0",
    "serve-favicon": "^2.5.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "compression-webpack-plugin": "^3.1.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^2.1.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.27",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.45.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-pwa-manifest": "^4.2.0",
    "workbox-webpack-plugin": "^5.0.0"
  }
}
